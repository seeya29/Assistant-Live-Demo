{
  \"version\": 2,
  \"builds\": [
    {
      \"src\": \"api/main.py\",
      \"use\": \"@vercel/python\"
    }
  ],
  \"routes\": [
    {
      \"src\": \"/api/(.*)\",
      \"dest\": \"/api/main.py\"
    },
    {
      \"src\": \"/(.*)\",
      \"dest\": \"/api/main.py\"
    }
  ],
  \"env\": {
    \"PYTHONPATH\": \".\"
  },
  \"functions\": {
    \"api/main.py\": {
      \"maxDuration\": 30
    }
  }
}